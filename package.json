{
  "name": "dunga-dunga-backend",
  "version": "1.0.0",
  "description": "Decked Out 2 backend API",
  "main": "dist/index.js",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=18.19.0 <19"
  },
  "scripts": {
    "start": "pm2 start ecosystem.config.json --no-daemon",
    "start:dev": "pm2 start ecosystem.config.json --no-daemon --env dev",
    "start:build": "tsc --build && pm2 start ecosystem.config.json --no-daemon",
    "compile": "tsc --build",
    "compile:watch": "tsc --build --watch",
    "pre:dev": "cross-env NODE_ENV=development nodemon --delay 2 --experimental-modules --es-module-specifier-resolution=node dist/index.js",
    "dev2": "concurrently --kill-others \"yarn compile:watch\" \"yarn pre:dev\"",
    "dev": "tsc --build && node dist/index.js",
    "dev:watchold": "cross-env NODE_ENV=development nodemon --delay 2 -w src --experimental-modules --es-module-specifier-resolution=node --exec yarn run dev",
    "dev:watch": "tsc-watch --noClear -p ./tsconfig.json --onSuccess \"yarn run start:dev\"",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --check **/*.ts",
    "prettier:fix": "prettier --write **/*.ts",
    "docker:prod": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml up",
    "docker:dev:sync": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml up --force-recreate",
    "docker:dev": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml up -d",
    "docker:dev:logs": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml logs -f",
    "docker:db": "docker-compose -f docker-compose.db.yml up",
    "docker:build": "docker build -f Dockerfile -t ghcr.io/trackedout/dunga-dunga-backend:latest .",
    "docker:push": "docker push ghcr.io/trackedout/dunga-dunga-backend:latest",
    "release": "standard-version && git push --follow-tags origin master",
    "commit": "git add -A && cz"
  },
  "keywords": [
    "node",
    "typescript",
    "node.js",
    "express",
    "rest",
    "api",
    "mongodb",
    "mongoose",
    "es6",
    "es7",
    "es8",
    "es9",
    "docker",
    "joi",
    "eslint",
    "prettier"
  ],
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@jest/globals": "^29.7.0",
    "@ryansonshine/commitizen": "^4.2.8",
    "@ryansonshine/cz-conventional-changelog": "^3.3.4",
    "@types/compression": "^1.7.2",
    "@types/cors": "2.8.17",
    "@types/express": "^4.17.13",
    "@types/express-rate-limit": "^6.0.0",
    "@types/morgan": "^1.9.3",
    "@types/node": "^20.14.8",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "@typescript-eslint/parser": "^7.13.1",
    "concurrently": "^8.2.2",
    "eslint": "^9.5.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^28.6.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-security": "^3.0.1",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "node-mocks-http": "^1.15.0",
    "nodemon": "^3.1.4",
    "tsc-watch": "^6.2.1",
    "prettier": "^3.3.2",
    "standard-version": "^9.3.2",
    "ts-jest": "^29.1.5",
    "typescript": "^5.5.2"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "discord.js": "^14.15.3",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-mongo-sanitize": "^2.1.0",
    "express-rate-limit": "^7.3.1",
    "helmet": "^7.1.0",
    "http-status": "^1.7.4",
    "joi": "^17.13.3",
    "mongoose": "^8.4.3",
    "morgan": "^1.10.0",
    "pm2": "^5.4.1",
    "rcon-client": "^4.2.4",
    "swagger-jsdoc": "^6.1.0",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^10.0.0",
    "winston": "^3.13.0",
    "xss-clean": "^0.1.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/@ryansonshine/cz-conventional-changelog"
    }
  }
}
